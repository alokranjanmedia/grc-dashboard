{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    },
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "functions": {
    "api/auth/github/callback.js": {
      "maxDuration": 30
    }
  },
  "env": {
    "GITHUB_CLIENT_ID": "Ov23li8cKR03tDtZyuoS",
    "GITHUB_CLIENT_SECRET": "bd106c8d3ae507a1f885ed51ff89f837ec66ae20"
  }
}
